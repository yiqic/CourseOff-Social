{
	"manifest_version": 2,

	"name": "CourseOffSocial",
	"description": "To view friends who are taking/have taken the same class with you",
	"version": "1.0",
	"author": "Jialiang (Jason) Zhang, Yiqi (Victor) Chen, Haoli Du", 

	"content_scripts": [
	    {
	      "matches": ["https://gatech.courseoff.com/*"],
	      "css": ["app.css"],
	      "js": ["jquery-1.11.2.min.js", 
	      	"dataparser.js", 
	      	"friendservice.js", 
	      	"app.js"
	      ]
	    }
	],

	"background": {
		"scripts": ["background.js"], 
		"persistent": false
	},

	"page_action": {
		"default_icon": "favicon19.png"
	},

	"permissions": [
		"tabs", "https://gatech.courseoff.com/*"
	]
}